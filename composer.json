{
  "name": "olliefordandco/amaranth",
  "description": "Laravel engine to power directory listing websites",
  "keywords": ["directory", "laravel"],
  "license": "UNLICENSED",
  "homepage": "https://www.ollieford.co.uk/",
  "authors": [
    {
      "name": "Ruben Madila",
      "email": "ruben@ollieford.co.uk"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "guzzlehttp/guzzle": "~5.0|~6.0",
    "illuminate/contracts": "~5.0",
    "illuminate/http": "~5.0",
    "illuminate/support": "~5.0",
    "jackiedo/dotenv-editor": "1.*",
    "olliefordandco/gatekeeper": "*",
    "olliefordandco/ui-manager": "*",
    "spatie/laravel-medialibrary": "^7.0.0",
    "php-ffmpeg/php-ffmpeg": "dev-master",
    "intervention/imagecache": "dev-master",
    "league/flysystem-aws-s3-v3": "dev-master"
  },
  "require-dev": {
    "mockery/mockery": "~0.9",
    "phpunit/phpunit": "~4.0|~5.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.3-dev"
    },
    "laravel": {
      "providers": [
        "OllieFordandCo\\Amaranth\\AmaranthServiceProvider",
        "Jackiedo\\DotenvEditor\\DotenvEditorServiceProvider",
        "OllieFordandCo\\UI\\UIServiceProvider",
        "OllieFordandCo\\GateKeeper\\Providers\\GateKeeperServiceProvider",
        "Spatie\\MediaLibrary\\MediaLibraryServiceProvider"
      ],
      "aliases": {
        "DotenvEditor": "Jackiedo\\DotenvEditor\\Facades\\DotenvEditor",
        "GateKeeper": "OllieFordandCo\\GateKeeper\\Providers\\GateKeeperServiceProvider",
        "UI": "OllieFordandCo\\UI\\UIServiceProvider",
        "Me": "OllieFordandCo\\UI\\UIServiceProvider"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "OllieFordandCo\\Amaranth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../gatekeeper"
    },
    {
      "type": "path",
      "url": "../ui-manager"
    }
  ]
}